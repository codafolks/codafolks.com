---
import Head from "./components/main-head.astro";
import Header from "./components/main-header.astro";
import Footer from "./components/main-footer.astro";
import Title from "@/components/web/title.astro";
import Prose from "@/components/web/prose.astro";
const { frontmatter } = Astro.props;
---

<html lang="en">
  <Head />
  <body class="bg-slate-900">
    <Header />
    <main class="text-white p-8">
      <div class="mx-auto w-full max-w-7xl">
        <Prose>
          <slot />
        </Prose>
        <p>Written on: {frontmatter.date}</p>
      </div>
    </main>
    <Footer />
  </body>
</html>
